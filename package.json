{
  "name": "zapzap-bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@open-wa/wa-automate": "^4.8.2",
    "axios": "^0.21.1",
    "concurrently": "^6.2.0",
    "g-i-s": "^2.1.6",
    "image-data-uri": "^2.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.31"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.0",
    "babel-jest": "^27.0.6",
    "fast-glob": "^3.2.7",
    "jest": "^27.0.6",
    "nodemon": "^2.0.11",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "concurrently yarn:start*",
    "start:bot": "ts-node src/index",
    "start:panel": "cd src/panel && yarn start",
    "dev": "./startdev.sh",
    "dev:bot": "nodemon --watch 'src/**' --ext 'ts,json'  --exec 'ts-node src/index.ts' --config './nodemon.json'",
    "dev:panel": "cd src/panel && yarn start",
    "test": "jest",
    "test:unit": "jest --passWithNoTests --runInBand"
  }
}
